<form action="" method="get">
  <div class="form-group row">
    <label for="id" class="col-sm-2 col-form-label">
      <input class="cid" type="checkbox" name="id" value="true">
    </label>
    <div class="col-sm-10">
      <input type="text" class="form-control " name="id" value="<%= query.id %>">

    </div>

  </div>
  <div class="form-group row">
    <label for="nama" class="col-sm-2 col-form-label">
      <input class="cnama" type="checkbox" name="nama" value="true">
    </label>
    <div class="col-sm-10">
      <input type="text" class="form-control " name="nama" value="<%= query.nama %>">

    </div>

  </div>
  <div class="form-group row">
    <label for="umur" class="col-sm-2 col-form-label">
      <input class="cumur" type="checkbox" name="umur" value="true">
    </label>
    <div class="col-sm-10">
      <input type="text" class="form-control " name="umur" value="<%= query.umur %>">

    </div>

  </div>
  <div class="form-group row">
    <label for="tinggi" class="col-sm-2 col-form-label">
      <input class="ctinggi" type="checkbox" name="tinggi" value="true">
    </label>
    <div class="col-sm-10">
      <input type="text" class="form-control " name="tinggi" value="<%= query.tinggi %>">

    </div>

  </div>
  <div class="form-group row">
    <label for="tanggallahi" class="col-sm-2 col-form-label">
      <input class="ctanggallahi" type="checkbox" name="tanggallahir" value="true">
    </label>
    <div class="col-sm-10">
      <input type="text" class="form-control " name="startdate" value="<%= query.startdate %>">
      <input type="text" class="form-control " name="enddate" value="<%= query.enddate %>">
    </div>

  </div>
  <div class="form-group row">
    <label for="ismenikah" class="col-sm-2 col-form-label">
      <input class="cismenikah" type="checkbox" name="ismenikah">
    </label>
    <div class="col-sm-10">
      <select class="" name="">
        <option value="true" <%= query.ismenikah ? ' selected' : ''%>></option>
        <option value="false" <%= query.ismenikah ? ' selected' : ''%>></option>

      </select>

    </div>

  </div>

</form>
